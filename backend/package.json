  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "test": "jest",
    "validate": "yarn test && yarn lint",
    "release": "npx np",
    "prestart": "yarn build",
    "start": "node lib/index.js",
    "dev": "nodemon"
  },
  "keywords": [],
  "files": [
    "lib/*.js",
    "lib/index.d.ts"
  ],
  "author": {
    "name": "David Brownman",
    "email": "beamneocube@gmail.com",
    "url": "https://davidbrownman.com"
  },
  "license": "ISC",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
